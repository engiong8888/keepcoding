{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "notify_type",
    "title",
    "content",
    "skip_type",
    "source"
  ],
  "properties": {
    "notify_type": {
      "$id": "#/properties/notify_type",
      "type": "string",
      "title": "The Notify_type Schema",
      "default": "",
      "examples": [
        "4"
      ],
      "enum": ["1","2","3","4"],
      "maxLength":1,
      "pattern": "^(.*)$"
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "推送标题"
      ],
      "maxLength":20,
      "pattern": "^(.*)$"
    },
    "content": {
      "$id": "#/properties/content",
      "type": "string",
      "title": "The Content Schema",
      "default": "",
      "examples": [
        "推送内容"
      ],
      "pattern": "^(.*)$"
    },
    "skip_type": {
      "$id": "#/properties/skip_type",
      "type": "string",
      "title": "The Skip_type Schema",
      "default": "",
      "examples": [
        "1"
      ],
      "enum": ["1", "2", "3", "4"],
      "maxLength":1,
      "pattern": "^(.*)$"
    },
    "source": {
      "$id": "#/properties/source",
      "type": "string",
      "title": "The Source Schema",
      "default": "",
      "examples": [
        "01"
      ],
      "maxLength":2,
      "pattern": "^(.*)$"
    },
    "time_to_live": {
      "$id": "#/properties/time_to_live",
      "type": "integer",
      "title": "The Time_to_live Schema",
      "default": 86400,
      "examples": [
        0
      ],
      "minimum":60,
      "maximum":604800
    },
    "skip_content": {
      "$id": "#/properties/skip_content",
      "type": "string",
      "title": "The Skip_content Schema",
      "default": "",
      "examples": [
        "跳转内容"
      ],
      "maxLength":1024,
      "pattern": "^(.*)$"
    },
    "network_type": {
      "$id": "#/properties/network_type",
      "type": "integer",
      "title": "The Network_type Schema",
      "default": "",
      "examples": [
        -1
      ],
      "minimum":-1,
      "maximum":1,
      "enum": [-1, 1],
      "pattern": "^(.*)$"
    }
  }
}