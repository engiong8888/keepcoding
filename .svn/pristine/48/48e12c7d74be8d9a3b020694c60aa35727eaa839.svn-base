/*新订单通知*/
.notice-box {
	border: 4px solid #0d87c2;
	height: 230px;
	background:#fff;
}
.notice-tit span {
	position: relative;
	display: inline-block;
	padding: 0 10px 0 20px;
	font-size: 20px;
	color: #fff;
	height: 36px;
	line-height: 32px;
	background: #0d87c2;
}
.notice-tit span:before {
	position: absolute;
	content: "";
	right: -38px;
	top: -6px;
	width: 38px;
	height: 42px;
	background: url('../images/notice_tit.png') no-repeat;
}
.notice-link {
	margin: 30px 0 42px;
	text-align: center;
}
.notice-link em {
	position: relative;
	float: left;
	margin: 0 26px;
	width: 42px;
	height: 42px;
	text-align: center;
	background: #0d87c2;
	border-radius: 100%;
}
.notice-link em .icon-file {
	position: absolute;
	left: 0;
	top: 0;
	bottom: 0;
	right: 0;
	margin: auto;
	color: #fff;
	line-height: 42px;
	font-size: 24px;
}
.notice-link a {
	float: left;
	color: #1a1a1a;
	line-height: 47px;
	vertical-align: middle;
	font-size: 20px;
	cursor: pointer;
}
.notice-link a:hover {
	color: #000;
}
.notice-link a span {
	color: #fe5000;
	font-family: Tahoma;
}
.notice-btn {
	margin: auto;
	width: 260px;
	height: 42px;
	border: 1px solid #0d87c2;
	color: #0d87c2;
	text-align: center;
	font-size: 18px;
	line-height: 42px;
	border-radius: 4px;
