@charset 'utf-8';
/* CSS reset */
body{background-color: #fff;overflow-x:hidden}
.wip-body{background-color: #f1f1f1;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td,strong,i,em{padding:0;margin:0;font-family:'Microsoft YaHei',Arial,'Helvetica Neue','SimSun';color:#3b3b3b}
table{border-collapse:collapse;border-spacing:0;}
a{text-decoration:none; color:#3b3b3b; outline:none;}/*此处待添加默认链接颜色*/
var,em,strong,i{font-style:normal;font-weight: normal;}
label{font-weight: normal;}
li{list-style:none;}
textarea{resize:none}
input{color: #606060;border: 1px solid #acacac;border-radius: 2px;}
input[type="radio"]{border: none}
input::-ms-clear{display:none;}
h1,h2,h3,h4,h5,h6{font-size:16px;font-weight:normal;}
.clearfix:after {content: "\200B";display: block;height: 0;overflow: hidden;clear: both;}
.clearfix {*zoom: 1;}

@media (min-width: 0px){
	.container{
		width: 100%;
	}
}
.sidebar {
	-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none; user-select:none;
}

/*登录页面*/
.login-page {
	width: 100%;
	height: 100%;
	background: #169bdb;
}
.login-box {
	position: absolute;
	top: 50%;
	left: 0;
	right: 0;
	padding: 0 30px;
	margin: -183px auto 0;
	width: 380px;
	height: 445px;
	background: #fff;

}
.login-box dl {
	position: absolute;
	top: -89px;
	left: 0;
	right: 0;
}
.login-box dd {
	margin: auto;
	display: block;
	width: 165px;
	height: 165px;
}
.login-box dt {
	margin-top: 25px;
	font-size: 30px;
	font-weight: normal;
	text-align: center;
}
.login-box  .form-horizontal {
	padding-top: 200px;
}
.login-box .form-control {
	height: 40px;
}
.login-box .btn {
	margin-top: 33px;
	width: 100%;
	height: 48px;
	color: #fff;
	font-size: 22px;
}


/*公共头部*/
.header {
	width: 100%;
	height: 46px;
	background: #fff;
	border-bottom: 1px solid #ccc; 
	z-index: 100;
}
.header ul {
	float: left;
}
.header ul li {
	position:relative;
	float: left;
	padding: 0 10px;
	color: #0a1130;
	font-size: 16px;
	height: 46px;
	line-height: 46px;
}
.header .switch ,.header p{
	float: right;
}
.header p {
	margin: 0 20px;
	line-height: 46px;
}
.header p i {
	margin-right: 5px;
}
.header p a {
	margin-left: 5px;
	color: #169bdb;
}
.header .switch {
	width: 46px;
	height: 46px;
	text-align: center;
	line-height: 55px;
	background: #169bdb;
	cursor: pointer;
}
.header .switch:hover {
	background: #0d87c2;
}
.header .switch .icon-off {
	color: #fff;
}
.header .hd-msg {
	position: relative;
	cursor: pointer;
}
.header .hd-msg a:hover {
	text-decoration: none;
}
.header .hd-msg .icon-bell-alt {
	position: relative;
}
.header .hd-msg:before {
	position: absolute;
	content: "";
	right: -19px;
	top: 0;
	bottom: 0;
	margin: auto;
	height: 15px;
	width: 1px;
	background: #5EB2DA;
}
.header .hd-msg .num {
	display: inline-block;
	margin-left: 3px;
	padding: 2px;
	min-width: 16px;
	height: 16px;
	font-size: 12px;
	background: #FE5000;
	color: #fff;
	text-align: center;
	line-height: 13px;
	border-radius: 100%;
}

/*内容左边栏*/
.sidebar-s, .sidebar {
	position: absolute;
	top: 0px;
	left: 0px;
	background-color: #222;
	z-index: 100;
}
.sidebar-s {
	display: none;
	left: 0px;
	width: 27px;
}
.sidebar {
	width: 190px;
}
.sidebar .side-toggler, .sidebar-s i {
	float: right;
	margin-top: 22px;
	width: 27px;
	height: 30px;
	text-align: center;
	line-height: 30px;
	background: #fff;
	cursor: pointer;
}
.sidebar .side-toggler,.sidebar-s i {
	border-radius: 2px 0 0 2px;
}
.sidebar-menu {
	border-bottom: 1px solid #464649;
}
.sidebar-menu .side-tit {
	display: block;
	position: relative;
	overflow: hidden;
	margin-left: 0px;
	padding: 0 0 0 30px;
	height: 40px;
	line-height: 40px;
	font-size: 16px;
	color: #fff;
	border-top: 1px solid #464649;
	transition: all 0s;
	cursor: pointer;
}
.sidebar-menu .side-tit.on {
	color: #fff;
	background: #2e2e2e;
}
.sidebar-menu .sub-list {
	position: relative;
}
.sidebar-menu .side-tit i {
	margin-right: 10px;
	color: #fff;
}
.sidebar-menu .sub-list li {
	position: relative;
	padding-left: 56px;
	height: 38px;
	line-height: 38px;
	color: #fff;
	font-size:14px;
	border-bottom: 1px solid #000;
    border-top: 1px solid #2B2B2B;
	cursor: pointer;
	-moz-transition: all 0.2s;
    -o-transition: all 0.2s;
    -webkit-transition: all 0.2s;
    transition: all 0.2s;
}
.sidebar-menu .sub-list li.on {
	color: #169bdb;
}
.sidebar-menu .sub-list li:hover {
	color: #169bdb;
	background: #454444; 
}
.sidebar-box {
	position: relative;
	margin-top: 90px;
	width:100%;
}
/*模拟滚动条公用样式*/
.overview { list-style: none; position: absolute; left: 0; top: 0;width: 100%; }
.thumb .end, .thumb { background-color: #3B3B40; }
.scrollbar { position: absolute; float: right; width: 6px;top: 0;right: 0;z-index: 1;visibility: hidden;transition: visibility .5s;-moz-transition: visibility .5s;-webkit-transition: visibility .5s;-o-transition: visibility .5s;}
.thumb { width: 6px; cursor: pointer; overflow: hidden; position: absolute; top: 0; left:0px;background: #3B3B40;border-radius: 5px;}
.thumb .end { overflow: hidden; height: 5px; width: 6px; visibility: hidden;}
.disable{ display: none; }
.noSelect { user-select: none; -o-user-select: none; -moz-user-select: none; -khtml-user-select: none; -webkit-user-select: none; }

.sidebar-box:hover .scrollbar {
	visibility: visible;
}
.sidebar-box .viewport{  
	overflow: hidden; 
	position: relative;
} 

/*中间内容区*/
.container {
	position: relative;
	padding: 17px 2px 0 0;
}
/*内容切换*/
.tab-titbox {
	position:relative;
	height: 40px;
	border-bottom: 1px solid #0d87c2;
}
.tab-tit {
	overflow: hidden;
	float: left;
	height: 40px;
}
.tab-titbox b {
	display: none;
	position: absolute;
	top: 0px;
	right: 3px;
	margin: 5px 5px 0 0;
	color: #169bdb;
	cursor: pointer;
}
.tab-titbox b:hover {
	color: #0d87c2;
}
.tab-tit li {
	position: relative;
	overflow: hidden;
	float: left;
	width: 132px;
	height: 39px;
	line-height: 39px;
	border-top: 1px solid #c1c1c1;
	border-left: 1px solid #c1c1c1;
	border-right: 1px solid #c1c1c1;
	background-color: #fff;
	text-align: center;
	color: #c1c1c1;
	font-size: 14px;
	border-radius: 5px 5px 0 0;
	cursor: pointer;
}
.tab-tit li:hover {
	background: #0d87c2;
	color: #fff;
	border: 1px solid #0d87c2;
}
.tab-tit li:hover em {
	background:url('../images/close_btn.png') 0 -46px no-repeat;
}
.tab-tit li a {
	color: #FFF;
}
.tab-tit span {
	display: inline-block;
	margin: 0 auto;
	height: 39px;
	line-height: 39px;
	width:100px;
	overflow: hidden;
	text-align: center;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.tab-tit em {
	position: absolute;
	right: 5px;
	top: 3px;
	width: 14px;
	height: 14px;
	line-height: 14px;
	color: #ddd;
	background:url('../images/close_btn.png') 0 0 no-repeat;
}
.tab-tit .on,.tab-tit .on:hover {
	color: #0d87c2;
	border: 1px solid #0d87c2;
	border-width: 1px 1px 0 1px;
	height: 41px;
	background: #fff;
}
.tab-tit .on em, .tab-tit .on:hover em {
	background:url('../images/close_btn.png') 0 -62px no-repeat;
}
.main-box{
	overflow-y:auto;
	padding: 15px;
	border: 1px solid #0d87c2;
	border-width: 0 1px 1px 1px;
	background: #fff;
	height:100%;
}
.con-box {
	display: none;
	position: relative;
	height: 100%;
	width: 100%;	
}
.con-box.on{
	display: block;
}

/*内容部分*/
.content {
	overflow: hidden;
	margin-left: 198px; 
}

/*欢迎页*/
.welcome-page {
	position: absolute;
	left: 0;
	bottom: 0;
	top: 0;
	right: 0;
	width: 469px;
	height: 351px;
	margin: auto;
}
.welcome-page img {
	display: block;
	margin: 0 auto;
	width:100%;
}
.copyright {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
