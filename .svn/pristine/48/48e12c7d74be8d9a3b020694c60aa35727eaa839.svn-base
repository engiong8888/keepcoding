}

/*订单中心*/
.order-cen-tit {
	margin-top: 15px;
    padding-top: 10px;
    background: #eee;
    height: 49px;
    border: 1px solid #d9d9d9;
}
.order-cen-tit ul {
	overflow: hidden;
    margin-left: -1px;
    height: 38px;
    width: 100%;
}
.order-cen-tit li {
	position: relative;
    float: left;
    margin-right: -1px;
    width: 130px;
    border: 1px solid #d9d9d9;
    border-bottom: none;
    line-height: 38px;
    border-radius: 3px 3px 0 0;
    text-align: center;
    font-size: 16px;
    cursor: pointer;
}
.order-cen-tit li.on {
	position: relative;
    height: 38px;
    color: #169bdb;
    background: #fff;
}
.order-cen-tit li.on:before {
	position: absolute;
    content: "";
    width: 100%;
    height: 2px;
    top: 0px;
    left: 0px;
    background: #169bdb;
    border-radius: 3px 3px 0 0;
}
.order-cen-con {
	display: none;
    padding: 13px 10px;
    border: 1px solid #d9d9d9;
    border-top: none;
}
.order-cen-con.on {
	display: block;
}
.laydate-icon, .laydate-icon-default, .laydate-icon-danlan, .laydate-icon-dahong, .laydate-icon-molv {
	height: 34px !important;
}
.layui-layer-title {
	border: 1px solid #eee;
	background-color: #fefefe;
    background: -moz-linear-gradient(top, #fff, #eee 100%);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), to(#eee));
}
.laydate_box,.laydate_top,.laydate_table,.laydate_bottom,.layui-layer-iframe iframe{
	box-sizing: content-box;
	-moz-box-sizing:content-box; /* Firefox */
	-webkit-box-sizing:content-box;
}
.order-date-list {
	overflow: hidden;
	margin-bottom: 15px;
	border: 1px solid #ccc;
	
	border-radius: 5px;
}
.order-date-list ul li {
	float: left;
	padding: 10px 0;
	width: 100px;
