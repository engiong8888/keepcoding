	font-size: 16px;
	color: #1a1a1a;
	background-color: #fefefe;
    background: -moz-linear-gradient(top, #fff, #eee 100%);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), to(#eee));
	border-right: 1px solid #ccc;
	cursor: pointer;
}
.order-date-list input.btn {
	float:right;
	margin: -49px 10px 0 0;
}
.order-date-list ul li span {
	display: block;
	margin-top: 3px;
	font-size: 14px;
}
.order-date-list li.on {
	color: #fe5000;
}
.inner-tit {
	margin: 20px 0;
}
.order-info-show {
	font-size: 16px;
}
.inner-tit span{
	margin-bottom: 5px;
	padding-bottom: 5px;
