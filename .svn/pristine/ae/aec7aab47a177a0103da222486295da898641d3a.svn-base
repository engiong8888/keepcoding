spring:
  application:
    name: gnw-push-iosmdm-consumer
  cloud:
    config:
      discovery:
        enabled: true
        service-id: gnw-pub-config-server
      username: pubuser
      password: pubConfig&^%
      fail-fast: true
      retry:
        max-attempts: 6
        multiplier: 1.1
        initial-interval: 1000
        max-interval: 2000
eureka:
  instance:
    lease-expiration-duration-in-seconds: 15 #服务过期时间配置,超过这个时间没有接收到心跳EurekaServer就会将这个实例剔除
    lease-renewal-interval-in-seconds: 5      #服务刷新时间配置，每隔这个时间会主动心跳一次，默认30s
  client:
    registerWithEureka: true            #是否将自己注册到Eureka服务中，默认为true
    fetchRegistry: true               #是否从Eureka中获取注册信息，默认为true
---
spring:
  profiles: xwb
  cloud:
    config:
      profile: xwb
eureka:
  client:
    serviceUrl:
      defaultZone: http://172.168.50.56:9000/eureka/,http://172.168.50.57:9000/eureka/
