@charset 'utf-8';
/* CSS reset */
body{background-color: #fff;overflow-x:hidden}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td,strong,i,em{padding:0;margin:0;font-family:'Microsoft YaHei',Arial,'Helvetica Neue','SimSun';font-size:14px;}
table{border-collapse:collapse;border-spacing:0;}
a{text-decoration:none; color:#3b3b3b; outline:none;}/*此处待添加默认链接颜色*/
var,em,strong,i{font-style:normal;font-weight: normal;}
li{list-style:none;}
input{color: #606060;border: 1px solid #acacac;border-radius: 2px;}
h1,h2,h3,h4,h5,h6{font-size:16px;font-weight:normal;}
.clearfix:after {content: "\200B";display: block;height: 0;overflow: hidden;clear: both;}
.clearfix {*zoom: 1;}

/*login*/
.login-top {
    background: #05c3ed;
    width: 100%;
    height: 400px;
}
.uiform {
    position: relative;
    background: rgb(255, 255, 255);
    margin: -105px auto auto;
    border: 1px solid rgb(231, 231, 231);
    width: 400px;
    height: 210px;
    text-align: center;
}
.form-group {
    position: relative;
    margin: 15px 0;
    zoom: 1;
}
.form-control {
    width: 280px;
    padding: 5px 10px 5px 35px;
    line-height: 24px;
    color: #333;
    border: 1px solid #CCC0C0;
    border-radius: 3px;
    -moz-transition: box-shadow 0.4s, border-color 0.4s;
    -o-transition: box-shadow 0.4s, border-color 0.4s;
    -webkit-transition: box-shadow 0.4s, border-color 0.4s;
    transition: box-shadow 0.4s, border-color 0.4s;
}
.form-group-name {
    margin-top: 35px;
}
.form-control:focus{
    outline: none;
    border-color: #02b8e0;
}
.btn-blue {
    display: block;
    margin: 28px auto 0;
    width: 60%;
    text-align: center;
    border-radius: 4px;
    line-height: 38px;
    background: #05c3ed;
    color: #FFF;
    border: none;
    cursor: pointer;
}
.btn-blue:hover {
    background: #02b8e0;
}
.form-group .log-icon {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 44px;
    width: 20px;
    height: 36px;
}
.form-group-name .log-icon {
    background: url('../images/username.png') center no-repeat;
}
.form-group-pwd .log-icon {
    background: url('../images/password.png') center no-repeat;
}
.log-head {
    position: absolute;
    top: -87px;
    width: 100%;
}
.log-head dt {
    position:absolute;
    left: 0;
    right: 0;
    width: 97px;
    height: 92px;
    margin: auto;
}
.log-head dd {
    position: absolute;
    width: 30px;
    height: 20px;
    top: 74px;
     background: url('../images/log_hand.png') center no-repeat;
}
.log-head dd.left {
    left: 90px;
}
.log-head dd.right {
    right: 90px;
}


/*index*/
.header {
    position:fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 50px;
    background-color: #F4F4F4;
    border-bottom: 1px solid #ddd;
    z-index: 100;
}
.hd-exit {
    position:absolute;
    top: 0;
    bottom: 0;
    right: 20px;
    width: 33px;
    background: url('../images/exit.png') center no-repeat;
    cursor: pointer;
}
.hd-exit:hover {
     background: url('../images/exit_on.png') center no-repeat;
}
.header img {
    margin: 7px 0 0 10px;
    height: 38px;
}
.container {
    position: relative;
}
.page-sidebar {
    position:fixed;
    top: 50px;
    left: 0;
    width: 200px;
    height: 400px;
    background: #3d3d3d;
    z-index: 100;
}
.sidebar-toggler {
    display: inline-block;
    margin-top: 15px;
    margin-left: 165px;
    width: 29px;
    height: 29px;
    background: url('../images/sidebar-toggler.jpg') no-repeat;
    cursor: pointer;
    opacity: 0.5;
    filter: alpha(opacity=50);
}
.page-sidebar-menu {
    margin-top: 21px;
    color: #fff;
}
.page-sidebar-menu dt img {
    display: block;
    margin: 0 auto 10px;
    width: 100px;
}
.page-sidebar-menu dt h3 {
    height: 40px;
    text-align: center;
    line-height: 40px;
    color: #fff;
    background: #05c3ed;
}
.page-sidebar-menu dd a {
    position: relative;
    display: block;
    padding-left: 32px;
    line-height: 40px;
    color: #fff;
    border-bottom: 1px solid #5c5c5c;
    cursor: pointer;
}
.page-sidebar-menu dd.on i{
    position: absolute;
    right: 0px;
    top: 8px;
    width: 8px;
    height: 25px;
    background-image: url("../images/sidebar-menu-arrow.png");
}
.page-sidebar-menu dd.on a {
    background-color: #000;
    color: #05c3ed;
}
.page-sidebar-menu dd a:hover {
    background-color: #333;
}
.page-content {
    position: relative;
    top: 50px;
    padding: 20px;
    padding-left: 220px;
}
.tit {
    padding-left: 10px;
    height: 40px;
    line-height: 40px;
    font-weight: bold;
    border-left: 4px solid #05c3ed;
    background: #f1f1f1;
}
.modify-btn{
    float: left;
    margin: 5px 0 5px 20px;
}
.modify-btn li {
	float: left;
    margin-left: 10px;
    padding: 0 15px;
    border: 1px solid #05c3ed;
    line-height: 28px;
    text-align: center;
    color: #05c3ed;
    font-size: 14px;
    background: #fff;
    cursor: pointer;
    border-radius: 3px;
}
.modify-btn li:hover,.modify-btn li.on {
	background: #05c3ed;
	color: #fff;
}
table a {
    display:inline-block;
    margin: 0 3px;
    padding: 1px 5px;
    text-align: center;
    line-height: 16px;
    font-size: 12px;
    color: #fff;
    border-radius: 4px;
}
table a.release {
    background: #46b8da;
}
table a.release:hover {
    background: #2faed4;
}
table a.delete {
    background: #d9534f;
}
table a.delete:hover {
    background: #d4342f;
}
.paging {
	float: right;
	margin-top: 20px;
	height: 32px;
}
.paging p {
	float: left;
	margin-right: 10px;
	line-height: 32px;
}
.paging a {
	float: left;
	margin-left: -1px;
	padding: 8px 12px;
	line-height: 14px;
	height: 14px;
	text-align: center;
	border: 1px solid #DDD;
}
.paging a:hover {
    background: #e9e9e9;
}
.modify-cont {
    position: relative;
    margin-bottom: 15px;
    padding:20px 15px 15px;
    background:#fff;
    border: 1px solid #DDD;
    border-radius: 5px;
}
.modify-cont.mask-box {
    margin: 0;
    padding: 30px;
    border: none;
    background:#fff;
}
.modify-input {
    float: left;
    margin: 5px 0;
}
.input-text {
    position: relative;
    margin-bottom: 10px;
}
.input-text span {
    margin-right: 10px;
    width: 80px;
    text-align: right;
}
.modify-cont input {
    padding: 5px;
    width: 188px;
    line-height: 18px;
}
.upload-box {
    height: 180px;
}
.input-upgrade input[type="radio"] {
    margin: -2px 3px 0 0;
    width: auto;
    vertical-align: middle;
}
.input-upgrade label {
    margin-right: 10px;
    cursor: pointer;
}
.modify-cont select {
    height: 32px;
    width: 200px;
}
.modify-input select {
     margin-right: 20px;
}
.modify-cont span {
    display: inline-block;
    font-size: 14px;
}
.input-button {
    display: block;
    margin: 30px auto 0;
    background: #05c3ed;
    text-align: center;
    height: 34px;
    line-height: 22px;
    color: #fff;
    border: 1px solid #02b8e0;
    border-radius: 5px;
    vertical-align: middle;
    cursor: pointer;
}
.input-button:hover{
    background: #02b8e0;
}
.input-area {
    position: absolute;
    left: 420px;
    top: 30px;
}
.input-area textarea{
    padding: 5px;
    width: 305px;
    height: 173px;
    word-wrap: break-word;
    white-space: normal;
    word-break: break-all;
    border: 1px solid #ACACAC;
    resize: none;
    border-radius: 2px;
}
.center {
	text-align:center
}
.layui-layer-title {
    font-weight:bold;
}
/*批量上传*/
#platformContainer {
    overflow: auto;
    height: 360px;
}
.platform-item  {
    margin-bottom: 1px;
}
.platform-item dt {
    position:relative;
    padding: 0 15px;
    font-size: 13px;
    font-weight: bold;
    line-height: 40px;
    background: #f1f1f1;
    cursor: pointer;
}
.platform-item dt i {
    position: absolute;
    right: 10px;
    top: 0;
    bottom: 0;
    margin: auto;
    width: 16px;
    height: 16px;
    background: url('../images/select_tips.png') center no-repeat;
    transition: all .3s;
    -webkit-transition: all .3s;
}
.platform-item.on dt i {
    transform:rotate(90deg);
    -ms-transform:rotate(90deg);     
    -moz-transform:rotate(90deg);    
    -webkit-transform:rotate(90deg); 
    -o-transform:rotate(90deg);
}
.platform-item dd {
    display: none;
    padding: 10px;
}
.platform-item .form-label {
    display: inline-block;
    margin: 2px 5px;
    width: 23%;
    cursor: pointer;
}
.form-label input[type="checkbox"] {
    display: inline-block;
    margin: -1px 5px 0 0;
    vertical-align: middle;
}
.application-sign {
	color:#888;
	font-size:17px;
	margin-top:50px;
	text-align:center;
}
.blue {
    color: #05c3ed;
}
.platform-item a {
    float: right;
    margin-right: 30px;
}
.state-icon {
    display: inline-block;
    vertical-align: middle;
    margin: -4px 0 0 5px;
    width: 16px;
    height: 16px;
}
.state-icon.success {
    background: url('../images/success_icon.png') center no-repeat;
}
.state-icon.error {
    background: url('../images/erro_icon.png') center no-repeat;
}