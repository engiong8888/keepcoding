<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test">



    <!-- 查询华为配置 -->
    <select id="query" parameterType="com.alibaba.fastjson.JSONObject" resultType="java.util.Map">
       SELECT * FROM user t
       WHERE 1 = 1

   </select>

    <select id="queryCount" parameterType="com.alibaba.fastjson.JSONObject" resultType="java.lang.Integer">
       SELECT COUNT(*) FROM user t
       WHERE 1 = 1

   </select>
</mapper>